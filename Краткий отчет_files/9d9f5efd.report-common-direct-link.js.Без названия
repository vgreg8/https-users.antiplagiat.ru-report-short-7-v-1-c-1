$(document).ready(function(){$("#report-link, #report-link-short, .report-direct-link").on("click",function(a){if(!$(this).attr("data-open")){a.preventDefault(),copyToClipboard($(this).attr("href"));var b=new DialogHelper;b.modalHtml('<p class="complete">'+$.localize_report("The link has been copied to clipboard","~/wwwroot/js/report/common/reportDirectLink.js")+"</p><p>"+$.localize_report("You can share it with other users.","~/wwwroot/js/report/common/reportDirectLink.js")+"</p>")}})});